    "def train_format(sample):    \n",
    "    prompt = f\"### Instruction: {sample['instruction']}\\n\"\n",
    "    if sample['input']:\n",
    "        prompt += f\"### Input: {sample['input']}\\n\"\n",
    "    prompt += f\"### Response: {sample['output']}\"\n",
    "    return {\"train\": prompt}"
   ]
